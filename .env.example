# Environment Variables for Firebase
# Copy this file to .env.local and fill in your Firebase configuration

PLASMO_PUBLIC_FIREBASE_API_KEY=your-api-key
PLASMO_PUBLIC_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
PLASMO_PUBLIC_FIREBASE_DATABASE_URL=https://your-project-default-rtdb.firebaseio.com/
PLASMO_PUBLIC_FIREBASE_PROJECT_ID=your-project-id
PLASMO_PUBLIC_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
PLASMO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=123456789
PLASMO_PUBLIC_FIREBASE_APP_ID=your-app-id
PLASMO_MEASUREMENT_ID=G-XXXXXXXXXX
